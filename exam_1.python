import re

def extract_variables_and_values(text):
    # 주어진 정규표현식 패턴
    pattern = re.compile(r'(\w+)=([^=,]+)')
    # 정규표현식에 맞게 변수와 값을 추출하여 리스트에 저장
    results = re.findall(pattern, text)
    return results

